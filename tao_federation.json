    "rules": {
        "stealth_field": {
            "name": "Stealth Field",
            "description": "Once per round, when an enemy unit targets a friendly _Infantry_ unit within 3\" suffer -1 Accuracy. This may be used one additional time per round for each model with this rule.",
            "points": 5
        },
        "pulse_shield": {
            "name": "Pulse Shield",
            "description": "Once per round, when an enemy unit targets a friendly _Infantry_ unit within 3\" roll a die for each hit that unit takes. On a 3 or less that hit is prevented. This may be used one additional time per round for each model with this rule.",
            "points": 5
        },
        "commander_tactics": {
            "name": "Commander Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Infantry_ or _Heavy Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * Re-roll failed Initiative tests of 9+",
            "points": ["rule.x", {"multiply": 5}]
        },
        "warrior_tactics": {
            "name": "Warrior Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * Re-roll failed Initiative tests of 9+",
            "points": ["rule.x", {"multiply": 5}]
        },
        "savage_tactics": {
            "name": "Savage Tactics",
            "inputs": ["x"],
            "description": "When this unit is activated, choose up to X friendly _Savages_ _Infantry_ units within 6\" to receive one of the following bonuses until the end of the round: \n * Re-roll failed Accuracy tests of 9+ \n * Re-roll failed Strength tests of 9+",
            "points": ["rule.x", {"multiply": 5}]
        },
        "attached": {
            "name": "Attached",
            "description": "This model is attached to its hosting unit and counts as being embarked but does not take up any transport space and may shoot while embarked."
        },
        "spotting_laser": {
            "name": "Spotting Laser",
            "description": "Any time during this model's activation, pick an enemy unit within 36\" and roll a D10. On a 5 or less the enemy unit gains 1 spotting marker. When a friendly unit shoots at that target they may spend spotting markers to do the following: \n * One Marker: Re-roll hit rolls of 9+ \n * Two Markers: +1 to hit \n\n\n These effects are cumulative and may be activated multiple times.",
            "points": 5
        },
        "homing_beacon": {"name": "Homing Beacon", "description": "Friendly units deploying using Ambush deployment may ignore enemy units when deploying within 6\" of this model and automatically pass Initiative tests to Ambush.", "points": 5},
        "inhibitor": {"name": "Inhibitor", "description": "Enemy units suffer -2\" of movement with any Charge actions targeting friendly units within 3\".", "points": 5},
        "tactical_drone": {
            "name": "Tactical Drone",
            "description": "Whenever a friendly _Infantry_ model within 6\" is killed or loses a Wound(x), roll a D10. On a 3 or less, that loss is prevented.",
            "points": 5
        },
        "recon_drone": {"name": "Recon Scope", "description": "Friendly models within 3\" may ignore cover bonuses.", "points": 2},
        "accelerator": {"name": "Accelerator", "description": "Friendly models within 3\" add +3\" to the short and long ranges of all weapons.", "points": 5},
        "threat_detector_suit": {"name": "Threat Detector", "description_short": "+1 Initiative", "description": "This model gets +1 Initiative.", "points": 5},
        "hover_drone": {"name": "Hover Drone", "hidden": "true", "description": "This model gets +2\" Move and Fly.", "description_short": "+4\" Move, Fly", "points": 5},
        "protection": {"name": "Protection", "description": "Friendly _Infantry_ units within 3\" get +1 Save.", "points": 5}
    },
    "strategies": {
        "covering_fire": {
            "name": "Covering Fire",
            "phase": "activation",
            "flavor": "Members of the Federation are tirelessly drilled on covering one another in the event that one unit is overrun.",
            "description": "Use when a friendly _Infantry_ or _Heavy Infantry_ unit is Charged. One friendly _Infantry_ or _Heavy Infantry_ unit within 6\" may take an Initiative test and attempt to react to the charging unit.",
            "cost": 1
        },
        "preservation_protocols": {
            "name": "Preservation Protocols",
            "phase": "activation",
            "flavor": "Federation drones are programmed to protect their creators at all costs.",
            "description": "Use after an _Infantry_ or _Heavy Infantry_ unit has been hit by a ranged weapon. For this attack, you can roll a D10 each time an attack made with a ranged weapon hits that unit. If there is a friendly _Drone_ unit within 6\" of it, and the _Drone_ unit is closer to the attacking model than the target unit. On an 8 or less a hit is transferred to the _Drone_ unit instead of the original target.",
            "cost": 2
        },
        "spotting_laser_uplink": {
            "name": "Spotting Laser Uplink",
            "phase": "activation",
            "flavor": "Some units equipped with spotting lasers are connected to a laser-uplink which boosts the signal of nearby spotting lasers.",
            "description": "Use after a unit successfully marks a unit with a spotting laser. You may place one additional spotting marker on the target.",
            "cost": 1
        }
    },
    "powers": {
        "fortify": {"name": "Courage", "description": "Target friendly unit within 12\" gets +1 Courage until the of the round.", "charge": 8},
        "steady": {"name": "Steady", "description": "Target friendly unit within 12\" gets +2 Initiative until the end of the round.", "charge": 8},
        "calm": {"name": "Calm", "description": "Target enemy unit within 18\" suffers -1 Strength with its next attack.", "charge": 6},
        "inferno": {"name": "Inferno", "description": "Target enemy unit within 12\" takes 3 hits with AP 3.", "charge": 6},
        "terror": {"name": "Terror", "description": "Target enemy unit within 12\" immediately takes 3 Shock.", "charge": 7},
        "storm": {"name": "Storm", "description": "Target enemy unit within 12\" takes 3 hits with AP 2.", "charge": 7}
    }
